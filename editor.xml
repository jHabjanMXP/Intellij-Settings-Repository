<application>
  <component name="CodeInsightSettings">
    <option name="INSERT_BRACE_ON_ENTER" value="false" />
  </component>
  <component name="DaemonCodeAnalyzerSettings" profile="Hepra Inspections" />
  <component name="EditorSettings">
    <option name="USE_SOFT_WRAPS" value="CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
    <pattern useCustomColors="true" pattern="\b(todo-jernej)\b.*">
      <option name="FOREGROUND" value="2dc0b1" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern useCustomColors="true" pattern="\b(todo-jernej-e)\b.*">
      <option name="FOREGROUND" value="c03e46" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
    <pattern icon="question" useCustomColors="true" pattern="\b(check)\b.*">
      <option name="FOREGROUND" value="47c045" />
      <option name="FONT_TYPE" value="2" />
      <option name="ERROR_STRIPE_COLOR" value="977ab" />
    </pattern>
  </component>
</application>